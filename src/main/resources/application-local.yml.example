server:
  port: 8080

spring:
  datasource:
    url: jdbc:postgresql://localhost:5432/tms_db
    username: postgres
    password: YOUR_PASSWORD_HERE
    driver-class-name: org.postgresql.Driver

  jpa:
    hibernate:
      ddl-auto: none
    show-sql: false

management:
  endpoints:
    web:
      exposure:
        include: health
